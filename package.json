{
  "name": "boris",
  "version": "0.0.1",
  "description": "This is a web application designed to deliver apocalypse-preparation training activities to participants.",
  "main": "backend/backend-server.ts",
  "scripts": {
    "pretest": "NODE_ENV=test ./node_modules/.bin/ts-node backend/db/reset-db.ts",
    "test": "NODE_ENV=test jest",
    "migrate": "ts-node backend/db/migrate.ts",
    "server": "ts-node --nolazy --inspect-brk=9229 backend/backend-server.ts",
    "build": "cd frontend && webpack",
    "watch": "cd frontend && webpack --watch",
    "lint": "tslint -p ."
  },
  "author": "From the Storm",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "body-parser": "~1.18.2",
    "connect-redis": "~3.1.0",
    "express": "~4.16.2",
    "express-session": "~1.15.6",
    "express-ws": "~3.0.0",
    "jsonrpc-websocket-client": "~0.2.0",
    "massive": "~4.5.0",
    "nodemailer": "~4.3.1",
    "nodemailer-mock-transport": "~1.3.0",
    "nodemailer-sparkpost-transport": "~2.0.0",
    "passport": "~0.4.0",
    "passport-unique-token": "~0.1.4",
    "pug": "~2.0.0-rc.4",
    "redis": "~2.8.0",
    "ts-node": "~3.3.0",
    "typescript": "~2.6.1",
    "validator": "~9.1.1",
    "whiskers": "~0.4.0"
  },
  "devDependencies": {
    "@types/connect-redis": "0.0.7",
    "@types/express": "~4.0.39",
    "@types/express-session": "~1.15.6",
    "@types/jest": "~22.0.0",
    "@types/massive": "~3.0.3",
    "@types/nodemailer": "~4.3.2",
    "@types/pg": "~7.4.1",
    "@types/react": "~16.0.28",
    "@types/react-dom": "~16.0.2",
    "@types/react-redux": "~5.0.10",
    "@types/redis": "~2.8.1",
    "@types/selenium-webdriver": "~3.0.8",
    "@types/ws": "~3.2.0",
    "awesome-typescript-loader": "~3.4.1",
    "bind-decorator": "~1.0.11",
    "chromedriver": "~2.34.0",
    "css-loader": "~0.28.7",
    "extract-text-webpack-plugin": "~3.0.2",
    "immutable": "4.0.0-rc.9",
    "jest": "~22.0.4",
    "node-sass": "~4.6.0",
    "react": "~16.0.0",
    "react-dom": "~16.0.0",
    "react-redux": "~5.0.6",
    "redux": "~3.7.2",
    "redux-thunk": "~2.2.0",
    "sass-loader": "~6.0.6",
    "selenium-webdriver": "~3.6.0",
    "source-map-loader": "~0.2.2",
    "ts-jest": "~22.0.3",
    "tslint": "~5.8.0",
    "uglify-es": "~3.1.5",
    "uglifyify": "~3.0.4",
    "webpack": "~3.8.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "test.*\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
