{
  "name": "boris",
  "version": "0.0.1",
  "description": "This is a web application designed to deliver apocalypse-preparation training activities to participants.",
  "main": "backend/backend-server.ts",
  "scripts": {
    "pretest": "NODE_ENV=test ./node_modules/.bin/ts-node backend/db/reset-db.ts",
    "test": "NODE_ENV=test jest",
    "migrate": "ts-node backend/db/migrate.ts",
    "server": "node -r ts-node/register --nolazy --inspect-brk=9229 backend/backend-server.ts",
    "build": "cd frontend && webpack",
    "watch": "cd frontend && webpack --watch",
    "lint": "tslint -p .",
    "interactive-multisession": "./node_modules/.bin/ts-node integration/interactive-multisession.ts"
  },
  "author": "From the Storm",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "body-parser": "~1.18.2",
    "connect-redis": "~3.3.3",
    "express": "^4.16.3",
    "express-session": "~1.15.6",
    "express-ws": "~3.0.0",
    "js-interpreter": "^1.4.6",
    "js-yaml": "^3.12.0",
    "json-rpc-peer": "~0.15.3",
    "massive": "~4.7.2",
    "nodemailer": "~4.6.4",
    "nodemailer-mock-transport": "~1.3.0",
    "nodemailer-sparkpost-transport": "~2.0.0",
    "passport": "~0.4.0",
    "passport-unique-token": "~0.1.4",
    "pug": "^2.0.3",
    "redis": "~2.8.0",
    "validator": "~10.0.0",
    "whiskers": "~0.4.0"
